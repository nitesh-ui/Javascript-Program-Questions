var string ="shiprasingh";
for(let i = 0; i < string.length; i++){
  var count = 0;
  for(j = 0; j < string.length; j++) {
    if(string[i] === string[j] && i > j) {
      break;
    }
    if(string[i] === string[j]) {
      count++
    }
  }
  if(count > 0) {
    console.log(`${string[i]} occurs ${count} times`);
  }
}
// Output 
s occurs 2 times
h occurs 2 times
i occurs 2 times
p occurs 1 times
r occurs 1 times
a occurs 1 times
n occurs 1 times
g occurs 1 times
